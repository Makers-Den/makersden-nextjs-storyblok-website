      "title": {
        "type": "text",
        "description": "This field holds the title of the web page, which appears in the browser tab and is used by search engines for the page's title in search results.\n\nEssential for SEO assessments and verifying that the correct page title is displayed depending on the context or user interaction within the application. Also used in automated tests to confirm page navigation accuracy.",
        "tooltip": true,
        "id": "iOHqcU4mRv2NzBMjTfPexQ"
      },
      "description": {
        "type": "textarea",
        "description": "It typically provides a brief summary of the page's content, used by search engines for page previews in search results.\n\nCrucially used in SEO analysis to ensure the meta description is effectively optimized for target keywords and accurately reflects the page content. Also used in UI testing to verify if the meta description adheres to best practices regarding length and relevance.",
        "tooltip": true,
        "id": "YzcWfGtOT72R2-y4oXNX4Q"
      },
      "nonIndexable": {
        "type": "boolean",
        "description": "Used in SEO compliance checks and automated testing to ensure that pages meant to be private or under development are not indexed by search engines. It helps in managing the visibility of web pages on the internet.",
