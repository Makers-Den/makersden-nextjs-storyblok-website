{
  "name": "storyblok-website",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=v20.11.1"
  },
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "eslint src --fix && pnpm run format",
    "lint:strict": "eslint --max-warnings=0 src",
    "typecheck": "tsc --noEmit --incremental false",
    "test:watch": "jest --watch",
    "test": "jest --passWithNoTests",
    "format": "prettier -w .",
    "format:check": "prettier -c .",
    "release": "standard-version",
    "push-release": "git push --follow-tags origin main",
    "prepare": "husky install",
    "proxy": "local-ssl-proxy --source 3010 --target 3000 --cert localhost.pem --key localhost-key.pem",
    "analyze-bundle": "ANALYZE=true pnpm run build",
    "pull-sb-components": "dotenv -e .env.local -- bash -c 'storyblok pull-components --space=$STORYBLOK_SPACE_ID --path ./storyblok-components/'",
    "build-sb-generator": "tsup-node src/lib/storyblok/storyblok-generate-ts/storyblokToTypescript.ts --outDir ./scripts-dist --format esm",
    "generate-sb-types": "dotenv -e .env.local -- bash -c 'storyblok login && pnpm run pull-sb-components && pnpm run build-sb-generator && cross-env STORYBLOK_SPACE_ID=$STORYBLOK_SPACE_ID STORYBLOK_ACCESS_TOKEN=$NEXT_PUBLIC_STORYBLOK_TOKEN node ./scripts/generateSbTypes.mjs'"
  },
  "dependencies": {
    "@mux/mux-player-react": "^3.3.0",
    "@next/bundle-analyzer": "^15.2.1",
    "@radix-ui/react-accordion": "^1.2.3",
    "@react-icons/all-files": "^4.1.0",
    "@storyblok/react": "^4.4.1",
    "@t3-oss/env-core": "^0.12.0",
    "@t3-oss/env-nextjs": "^0.12.0",
    "@types/node": "^22.13.9",
    "@vercel/og": "^0.6.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "jest-environment-jsdom": "^29.7.0",
    "local-ssl-proxy": "^2.0.5",
    "lucide-react": "^0.477.0",
    "next": "^15.2.2",
    "next-intl": "^4.0.1",
    "next-sitemap": "^4.2.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-wrap-balancer": "^1.1.1",
    "server-only": "^0.0.1",
    "storyblok-js-client": "^6.10.8",
    "storyblok-rich-text-react-renderer": "^2.9.2",
    "tailwind-merge": "^3.0.2",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@babel/core": "^7.26.9",
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
    "@jest/globals": "^29.7.0",
    "@svgr/webpack": "^8.1.0",
    "@swc/core": "^1.11.7",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/postcss": "^4.0.9",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/jest": "^29.5.14",
    "@types/json-schema": "^7.0.15",
    "@types/react": "^19.0.10",
    "@typescript-eslint/eslint-plugin": "^8.26.0",
    "@typescript-eslint/parser": "^8.26.0",
    "autoprefixer": "^10.4.20",
    "babel-jest": "^29.7.0",
    "babel-loader": "^10.0.0",
    "camelcase": "^8.0.0",
    "dotenv": "^16.4.7",
    "dotenv-cli": "^8.0.0",
    "eslint": "9.21.0",
    "eslint-config-next": "^15.2.1",
    "eslint-config-prettier": "^10.0.2",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-storybook": "^0.11.4",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "json-schema-to-typescript": "^15.0.4",
    "lint-staged": "^15.4.3",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "react-test-renderer": "^19.0.0",
    "standard-version": "^9.5.0",
    "storyblok": "^3.35.2",
    "tailwindcss": "^4.0.9",
    "ts-command-line-args": "^2.5.1",
    "ts-node": "^10.9.2",
    "tsup": "^8.4.0",
    "typescript": "^5.8.2"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,}": [
      "eslint --max-warnings=0",
      "prettier -w"
    ],
    "src/**/*.{json,css,scss,md}": [
      "prettier -w"
    ]
  },
  "resolutions": {
    "webpack": "^5"
  },
  "packageManager": "pnpm@9.1.1"
}
