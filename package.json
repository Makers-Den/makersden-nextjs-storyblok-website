{
  "name": "storyblok-website",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=v20.11.1"
  },
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "eslint src --fix && pnpm run format",
    "lint:strict": "eslint --max-warnings=0 src",
    "typecheck": "tsc --noEmit --incremental false",
    "test:watch": "jest --watch",
    "test": "jest --passWithNoTests",
    "format": "prettier -w .",
    "format:check": "prettier -c .",
    "release": "standard-version",
    "push-release": "git push --follow-tags origin main",
    "prepare": "husky install",
    "proxy": "local-ssl-proxy --source 3010 --target 3000 --cert localhost.pem --key localhost-key.pem",
    "analyze-bundle": "ANALYZE=true pnpm run build",
    "pull-sb-components": "dotenv -e .env.local -- bash -c 'npx storyblok pull-components --space=$STORYBLOK_SPACE_ID --path ./storyblok-components/'",
    "build-sb-generator": "tsup src/lib/storyblok/storyblok-generate-ts/storyblokToTypescript.ts --outDir ./scripts-dist",
    "generate-sb-types": "dotenv -e .env.local -- bash -c 'npx storyblok login && pnpm run pull-sb-components && pnpm run build-sb-generator && cross-env STORYBLOK_SPACE_ID=$STORYBLOK_SPACE_ID STORYBLOK_ACCESS_TOKEN=$NEXT_PUBLIC_STORYBLOK_TOKEN node ./scripts/generateSbTypes.js'"
  },
  "dependencies": {
    "@next/bundle-analyzer": "^13.1.5",
    "@react-icons/all-files": "^4.1.0",
    "@storyblok/react": "^3.0.10",
    "@t3-oss/env-core": "^0.10.1",
    "@t3-oss/env-nextjs": "^0.10.1",
    "@vercel/og": "^0.0.25",
    "clsx": "^1.2.1",
    "cross-env": "^7.0.3",
    "local-ssl-proxy": "^1.3.0",
    "next": "^14.2.3",
    "next-sitemap": "^3.1.24",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-wrap-balancer": "^0.4.1",
    "server-only": "^0.0.1",
    "storyblok-js-client": "^6.7.3",
    "storyblok-rich-text-react-renderer": "^2.9.2",
    "tailwind-merge": "^1.14.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@babel/core": "^7.21.3",
    "@commitlint/cli": "^13.2.1",
    "@commitlint/config-conventional": "^13.2.0",
    "@svgr/webpack": "^6.2.0",
    "@swc/core": "^1.3.40",
    "@tailwindcss/forms": "^0.5.3",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^15.0.7",
    "@types/react": "^18.3.2",
    "@types/tailwindcss": "^2.2.4",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "autoprefixer": "^10.4.12",
    "babel-jest": "^27.4.6",
    "babel-loader": "^8.2.4",
    "camelcase": "^6.3.0",
    "dotenv": "^16.0.3",
    "eslint": "8.57.0",
    "eslint-config-next": "^13.4.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-storybook": "^0.5.7",
    "husky": "^7.0.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.4.7",
    "json-schema-to-typescript": "^11.0.1",
    "lint-staged": "^11.2.6",
    "postcss": "^8.4.18",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "react-test-renderer": "^18.2.0",
    "sass": "^1.55.0",
    "standard-version": "^9.3.2",
    "storyblok": "^3.20.0",
    "tailwindcss": "^3.1.8",
    "ts-command-line-args": "^2.3.1",
    "ts-node": "^10.9.1",
    "tsup": "^6.4.0",
    "typescript": "^5.4.5"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,}": [
      "eslint --max-warnings=0",
      "prettier -w"
    ],
    "src/**/*.{json,css,scss,md}": [
      "prettier -w"
    ]
  },
  "resolutions": {
    "webpack": "^5"
  },
  "packageManager": "pnpm@9.0.6"
}
